{
  "version": 2,
  "name": "georgia-dashboard",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/",
      "dest": "/public/landing.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "DB_SERVER": "@db-server",
    "DB_PORT": "@db-port",
    "DB_USERNAME": "@db-username",
    "DB_PASSWORD": "@db-password",
    "DB_DATABASE": "@db-database",
    "TDS_VERSION": "7.0"
  },
  "regions": ["iad1"],
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.9",
      "maxDuration": 30,
      "memory": 1024
    }
  }
}
