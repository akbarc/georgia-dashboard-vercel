{
  "version": 2,
  "name": "georgia-dashboard",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/",
      "dest": "/public/landing.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "regions": ["iad1"],
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.9",
      "maxDuration": 30,
      "memory": 1024
    }
  }
}
